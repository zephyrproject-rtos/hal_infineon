    <ParamBool id="clkError" name="clkError" group="Internal" default="`${clkUsed ? getExposedMember(clkRsc, &quot;error&quot;) : true}`" visible="`${debug}`" editable="false" desc="" />
    <ParamRange id="clkFreq" name="clkFreq" group="Internal" default="`${!clkError ? getExposedMember(clkRsc, &quot;frequency&quot;) : 1}`" min="0" max="1000000000" resolution="1" visible="`${debug}`" editable="false" desc="" />

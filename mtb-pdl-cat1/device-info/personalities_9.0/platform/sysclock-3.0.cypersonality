    <ConfigInstruction value="/* Configure miscellaneous clocks */" include="`${is40ssrssAvailable || is40srssAvailable}`"  />
    <ConfigInstruction value="    #ifdef CY_CFG_SYSCLK_CLKTIMER_ENABLED" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="        Cy_SysClk_ClkTimerInit();" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    #endif" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKALTSYSTICK_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_ClkAltSysTickInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKPUMP_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_ClkPumpInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKBAK_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_ClkBakInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <!-- Configure default enabled clocks -->
    <ConfigInstruction value="/* Configure default enabled clocks */" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />
    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_ILO_ENABLED" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_IloInit();" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />

    <ConfigInstruction value="#ifndef CY_CFG_SYSCLK_IMO_ENABLED" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />
    <ConfigInstruction value="    #error the IMO must be enabled for proper chip operation" include="`${is40ssrssAvailable || is40srss_ver2or3Available || is40srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40ssrssAvailable || is40srss_ver2or3Available}`"  />
    <ConfigInstruction value="" include="`${is40ssrssAvailable || is40srss_ver2or3Available}`"  />
    <ConfigInstruction value="    #endif" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />

    <ConfigInstruction value="    #ifndef CY_CFG_SYSCLK_CLKHF0_ENABLED" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="        #error the CLKHF0 must be enabled for proper chip operation" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    #endif" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />

    <ConfigInstruction value="#endif /* defined(CY_DEVICE_SECURE) */" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_MFO_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_MfoInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKMF_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_ClkMfInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKPWR_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_ClkPwrInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <ConfigInstruction value="#if (!defined(CY_DEVICE_SECURE))" include="`${is40srssAvailable}`"  />
    <!-- Configure final flash wait states -->
    <ConfigInstruction value="    /* Set accurate flash wait states */" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    #if (defined (CY_CFG_PWR_ENABLED) &amp;&amp; defined (CY_CFG_SYSCLK_CLKHF0_ENABLED))" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="        Cy_SysLib_SetWaitStates(CY_CFG_PWR_USING_ULP != 0, CY_CFG_SYSCLK_CLKHF0_FREQ_MHZ);" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    #endif" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />

    <ConfigInstruction value="    /* Set accurate flash wait states */" include="`${is40srss_ver2or3Available}`"  />
    <ConfigInstruction value="    #if (defined (CY_CFG_PWR_ENABLED) &amp;&amp; defined (CY_CFG_SYSCLK_CLKHF1_ENABLED))" include="`${is40srss_ver2or3Available}`"  />
    <ConfigInstruction value="        Cy_SysLib_SetWaitStates(false, CY_CFG_SYSCLK_CLKHF1_FREQ_MHZ);" include="`${is40srss_ver2or3Available}`"  />
    <ConfigInstruction value="    #endif" include="`${is40srss_ver2or3Available}`"  />
    <ConfigInstruction value="" include="`${is40srss_ver2or3Available}`"  />

    <!-- Update System Core Clock values -->
    <ConfigInstruction value="    /* Update System Core Clock values for correct Cy_SysLib_Delay functioning */" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    SystemCoreClockUpdate();" include="`${is40srssAvailable}`"  />

    <!-- Finish to configure the ILO -->
    <ConfigInstruction value="    #ifndef CY_CFG_SYSCLK_ILO_ENABLED" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="        #ifdef CY_CFG_SYSCLK_CLKLF_ENABLED" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="        /* Wait 4 ILO cycles in case of unfinished CLKLF clock source transition */" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="        Cy_SysLib_DelayUs(200U);" include="`${is40srssAvailable}`"  /> <!--DRIVERS 3401-->
    <ConfigInstruction value="        #endif" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_IloDisable();" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_IloHibernateOn(false);" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="    #endif" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />

    <ConfigInstruction value="#endif /* (!defined(CY_DEVICE_SECURE)) */" include="`${is40srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srssAvailable}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKPATH0_ENABLED" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_ClkPath0Init();" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />

    <ConfigInstruction value="/* Configure and enable DPLLs */" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_DPLL_LP0_ENABLED" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_Dpll_Lp0_Init();" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_DPLL_LP1_ENABLED" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_Dpll_Lp1_Init();" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is22srssAvailable}`"  />

    <!-- Reset and configure platform clocks -->
    <ConfigInstruction value="/* Configure and enable FLL */" include="`${is40srss_ver2or3Available || is40ssrssAvailable}`"  />
    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_FLL_ENABLED" include="`${is40srss_ver2or3Available || is40ssrssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_FllInit();" include="`${is40srss_ver2or3Available || is40ssrssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40srss_ver2or3Available || is40ssrssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srss_ver2or3Available || is40ssrssAvailable}`"  />

    <ConfigInstruction value="Cy_SysClk_ClkHf0Init();" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />

    <ConfigInstruction value="/* Disable Unused Clock Sources */" include="`${is40ssrssAvailable}`"  />
    <ConfigInstruction value="#ifndef CY_CFG_SYSCLK_IHO_ENABLED" include="`${is40ssrssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_IhoDisable();" include="`${is40ssrssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40ssrssAvailable}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKFAST_0_ENABLED" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />
    <ConfigInstruction value="    Cy_SysClk_ClkFast_0_Init();" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />
    <ConfigInstruction value="#endif" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />
    <ConfigInstruction value="" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKFAST_1_ENABLED" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />
    <ConfigInstruction value="    Cy_SysClk_ClkFast_1_Init();" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />
    <ConfigInstruction value="#endif" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />
    <ConfigInstruction value="" include="`${(is40srss_ver2or3Available || is22srssAvailable) &amp;&amp; !is40srss_ver2or3withCM4}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKSLOW_ENABLED" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_ClkSlowInit();" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKMEM_ENABLED" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_ClkMemInit();" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is40srss_ver2or3Available || is22srssAvailable}`"  />

    <ConfigInstruction value="    Cy_SysClk_EcoPrescalerInit();" include="true" guard="defined(CY_CFG_SYSCLK_ECO_PRESCALER_ENABLED)" />
    <ConfigInstruction value="    Cy_SysClk_LpecoPrescalerInit();" include="`${isLpecoAvailable}`" guard="defined(CY_CFG_SYSCLK_LPECO_PRESCALER_ENABLED)" />
    <ConfigInstruction value="#endif /* defined(CORE_NAME_CM33_0) */" include="`${is22srssAvailable}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_CLKALTSYSTICK_ENABLED" include="true"  />
    <ConfigInstruction value="    Cy_SysClk_ClkAltSysTickInit();" include="true"  />
    <ConfigInstruction value="#endif" include="true"  />
    <ConfigInstruction value="" include="true"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_ECO_ENABLED" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_EcoSetFrequency(CY_CFG_SYSCLK_ECO_FREQ);" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is22srssAvailable}`"  />

    <ConfigInstruction value="#ifdef CY_CFG_SYSCLK_EXTCLK_ENABLED" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    Cy_SysClk_ExtClkSetFrequency(CY_CFG_SYSCLK_EXTCLK_FREQ);" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#endif" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is22srssAvailable}`"  />

    <!-- Update System Core Clock values -->
    <ConfigInstruction value="/* Update System Core Clock values for correct Cy_SysLib_Delay functioning */" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />
    <ConfigInstruction value="SystemCoreClockUpdate();" include="`${is40srss_ver2or3Available || is40ssrssAvailable || is22srssAvailable}`"  />

    <!-- Configure power options -->
    <ConfigInstruction value="#ifdef CY_CFG_PWR_ENABLED" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    #ifdef CY_CFG_PWR_INIT" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="        init_cycfg_power();" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    #else" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="        #warning Power system will not be configured. Update power personality to v1.20 or later." include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="    #endif /* CY_CFG_PWR_INIT */" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="#endif /* CY_CFG_PWR_ENABLED */" include="`${is22srssAvailable}`"  />
    <ConfigInstruction value="" include="`${is22srssAvailable}`"  />
  </ConfigFirmware>
</Personality>
